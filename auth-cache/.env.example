# Redis Configuration for Auth Cache Service
# Copy this file to .env and modify values as needed

# Redis server port
REDIS_PORT=6379

# Redis password for authentication
# Set a strong password in production
REDIS_PASSWORD=redis-secure-password-change-me

# Optional: Memory limit (default: 256mb)
# REDIS_MAXMEMORY=256mb

# Optional: Memory eviction policy (default: allkeys-lru)
# REDIS_MAXMEMORY_POLICY=allkeys-lru
